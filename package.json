{
  "name": "leopard-cli",
  "description": "Leopard CLI is a tool for setting up your dev environment quickly so that you can get coding.",
  "version": "0.0.0",
  "author": "NeverEnder4 @NeverEnder4",
  "bin": {
    "leopard": "./bin/run"
  },
  "bugs": "https://github.com/NeverEnder4/leopard-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-help": "^2.1.6",
    "cli-ux": "^5.2.1",
    "fs-extra": "^7.0.1",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "globby": "^8.0.2",
    "rimraf": "^2.6.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/NeverEnder4/leopard-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "leopard",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "NeverEnder4/leopard-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
